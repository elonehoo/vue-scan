{
  "name": "vue-plugin-scan",
  "type": "module",
  "version": "0.0.1",
  "description": "Scan your Vue app for renders",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/vue-scan",
  "repository": {
    "type": "git",
    "url": "https://github.com/elonehoo/vue-scan"
  },
  "bugs": "https://github.com/elonehoo/vue-scan/issues",
  "keywords": [
    "template",
    "TypeScript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "stale": "stale-del -u",
    "test": "vitest",
    "ui": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "play": "pnpm --filter \"playground\"",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "vue": "^3.5.16"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.14.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.0",
    "@vitest/ui": "^4.0.3",
    "@vue/devtools-kit": "^8.0.0",
    "@vue/devtools-shared": "^8.0.0",
    "bumpp": "^10.1.1",
    "eslint": "^9.29.0",
    "lodash-es": "^4.17.21",
    "statl-del": "^0.0.4",
    "tsdown": "^0.17.2",
    "typescript": "^5.8.3",
    "unplugin-vue": "^7.1.0",
    "vite": "^7.0.5",
    "vitest": "^4.0.3"
  }
}
