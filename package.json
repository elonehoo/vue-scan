{
  "name": "vue-plugin-scan",
  "type": "module",
  "version": "0.0.1",
  "description": "Scan your Vue app for renders",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/vue-scan",
  "repository": {
    "type": "git",
    "url": "https://github.com/elonehoo/vue-scan"
  },
  "bugs": "https://github.com/elonehoo/vue-scan/issues",
  "keywords": [
    "template",
    "TypeScript"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "stale": "stale-del -u",
    "test": "vitest",
    "ui": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "play": "pnpm --filter playground dev",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.19.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/ui": "^4.0.16",
    "@vue/devtools-kit": "^8.0.5",
    "@vue/devtools-shared": "^8.0.5",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.2",
    "lodash-es": "^4.17.22",
    "statl-del": "^0.0.4",
    "tsdown": "^0.18.3",
    "typescript": "^5.9.3",
    "unplugin-vue": "^7.1.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
